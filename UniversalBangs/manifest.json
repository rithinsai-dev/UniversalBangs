{
    "manifest_version": 3,
    "name": "Universal Bangs",
    "version": "1.0",
    "description": "Chrome extension for bang redirects",
    "omnibox": {
      "keyword": "!"
    },
    "host_permissions": ["*://*/*"],
    "permissions": ["storage", "tabs"],
    "background": {
      "service_worker": "background.js",
      "type": "module"
    }
  }
  
